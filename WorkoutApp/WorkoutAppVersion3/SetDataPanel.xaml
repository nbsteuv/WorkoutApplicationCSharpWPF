<UserControl x:Class="WorkoutAppVersion3.SetDataPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WorkoutAppVersion3"
             mc:Ignorable="d" 
             d:DesignHeight="32.612" Width="120">
    <Grid>
        <StackPanel HorizontalAlignment="Left" Height="33" VerticalAlignment="Top" Width="120" Orientation="Horizontal">
            <TextBox x:Name="WeightBox" TextWrapping="Wrap" Text="" Width="45" TextChanged="Weight_TextChanged" />
            <TextBox x:Name="RepBox" Margin="10,0,0,0" HorizontalAlignment="Left" Height="33" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="45" TextChanged="Rep_TextChanged" IsEnabled="False"/>
        </StackPanel>
    </Grid>
</UserControl>
